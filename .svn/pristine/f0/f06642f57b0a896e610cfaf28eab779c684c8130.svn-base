ul#menu {
	padding:0;
	margin:0;
}
ul#menu a {
	font-size:12px;
	font-weight: bold;
	text-decoration: none;
}
ul#menu, ul#menu ul, ul#menu li {
	margin: 0px;
	padding:5px 35px 5px 10px;
	list-style-type: none;
}
ul#menu li {
	float: left;
}
ul#menu li a {
	color: #ffffff;
	padding: 3px;
}
ul#menu li a:hover, ul#menu li a:active {
	color: #ffff00;
	background-color: #1C4A7B;/*when mouse is oover on main menu*/
}
ul#menu li a:active {
	color: #cccccc;
	background-color: #003366;
	border: 1px #ffffff solid;
}
ul#submenulist {
	display: none;
	background-color:#1C4A7B; /*when submenu list display*/
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-right-style: solid;
	border-bottom-style: solid;
	border-left-style: solid;
	border-right-color: #09C;
	border-bottom-color: #09C;
	border-left-color: #09C;
}
ul#submenulist li {
	float: none;
	padding:0px;
}
ul#submenulist li a {
	padding:0px 5px 3px 0px;
	margin: 0px;
}
ul#submenulist li a:hover {
	background-color:#4A7FB5; /*mouse over on submenu*/
}
ul#menu li:hover ul#submenulist {
	display: block;
	position: absolute;
	font-size: 8pt;
	margin-top:5px;
	padding:5px; /*submenu padding*/
}
ul#menu li:hover ul#submenulist li a {
	display: block;
	width: 100%; /*widhe of submenu*/
	border: none;
	padding: 5px 0px 5px 0px;
}
